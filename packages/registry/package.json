{
  "name": "@multi-game-engines/registry",
  "version": "0.1.0",
  "description": "Centralized engine metadata registry for multi-game-engines",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "data"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "devDependencies": {
    "@multi-game-engines/core": "workspace:*",
    "stockfish": "16.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "@multi-game-engines/core": "workspace:*"
  },
  "dependencies": {
    "@multi-game-engines/i18n-engines": "workspace:*",
    "zod": "^4.3.6"
  }
}
